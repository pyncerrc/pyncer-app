{
    "name": "pyncer/app",
    "type": "library",
    "description": "Pyncer library for making web applications.",
    "homepage": "https://pyncer.com/app",
    "license": "Unlicense",
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0.1",
        "psr/log": "^3.0.0",
        "pyncer/access": "^1.1",
        "pyncer/component": "^1.2",
        "pyncer/container": "^1.0.1",
        "pyncer/core": "^1.1",
        "pyncer/data": "^1.4",
        "pyncer/database": "^1.3",
        "pyncer/exception": "^1.0",
        "pyncer/http": "^1.2",
        "pyncer/iterable": "^1.1",
        "pyncer/image": "^1.1",
        "pyncer/log": "^1.1.0",
        "pyncer/routing": "^1.1",
        "pyncer/session": "^1.0",
        "pyncer/snyppet": "^1.2",
        "pyncer/source": "^1.0",
        "pyncer/utility": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Pyncer\\App\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/pyncerrc/pyncer-app/issues",
        "source": "https://github.com/pyncerrc/pyncer-app"
    },
    "extra" : {
        "branch-alias" : {
            "dev-main" : "1.5-dev"
        }
    }
